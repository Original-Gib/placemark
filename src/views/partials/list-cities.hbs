{{#each cities}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{cityName}}
    </h2>
    <a href="/city/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-folder-open"></i>
      </span>
    </a>
    <a href="/dashboard/deletecity/{{_id}}" class="button">
      <i class="fas fa-trash"></i>
    </a>
  </div>
{{/each}}