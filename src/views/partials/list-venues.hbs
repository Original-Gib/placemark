<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Venue</th>
      <th>Street</th>
      <th>Genre</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each city.venues}}
      <tr>
        <td>
          {{venueName}}
        </td>
        <td>
          {{street}}
        </td>
        <td>
          {{genre}}
        </td>
        <td>
            <a href="/city/{{../city._id}}/deletevenue/{{_id}}" class="ui icon button">
            <i class="fas fa-trash"></i>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>